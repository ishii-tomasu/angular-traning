	<!-- Google Code for Adwords users (registration) Conversion Page
		 In your html page, add the snippet and call
		 goog_report_conversion when someone clicks on the
		 chosen link or button. -->
	<script type="text/javascript">
		/*  */
		goog_snippet_vars = function() {
			var w = window;
			w.google_conversion_id = 855777422;
			w.google_conversion_label = "p8KpCNLrxXAQjsGImAM";
			w.google_remarketing_only = false;
		}
		/* Returns true if the email is valid */
		goog_validate_email = function() {
			var emailValidator = new RegExp("[\\.a-z\\d\\!\\#\\$\\%\\*\\/\\?\\|\\^\\{\\}\\`\\~\\&amp;\\'\\+\\-\\=\\_]+\\@[\\da-z]+([\\.-][\\da-z]+)*\\.[a-z]{2,6}$", "i");
			var email = document.getElementById("email").value;

			return emailValidator.test(email);
		}
		/* Returns true if the password is between 6 and 20 characters */
		goog_validate_password = function() {
			var password = document.getElementById("password").value;
			var passwordValidator = new RegExp("^[\\x20-\\x7E]{6,20}$", "i");

			return passwordValidator.test(password);
		}
		/* Returns true if the three date of birth inputs are not empty */
		goog_validate_dob = function() {
			// Just checks whether the input are empty; might want something better, if there are issues
			var yearValid = document.getElementById("dob-year").value !== "";
			var monthValid = document.getElementById("dob-month").value !== "";
			var dayValid = document.getElementById("dob-day").value !== "";

			// Inline script doesn't accept &amp;&amp;, so we add the booleans instead
			return yearValid + monthValid + dayValid == 3;
		}
		/* Returns true if the user has checked one of the gender radio buttons */
		goog_validate_gender = function() {
			return document.getElementById("female").checked || document.getElementById("male").checked;
		}
		/* Returns true if the user has checked the Agreement checkbox */
		goog_validate_agreement = function() {
			return document.getElementById("acceptagreement").checked || document.getElementById("male").checked;
		}
		/* Adds a validation filter, to make sure we don't count conversion for invalid signup attempts */
		goog_report_conversion_withvalidation = function() {
			// Inline script doesn't accept &amp;&amp;, so we add the booleans instead
			if(goog_validate_email() + goog_validate_password() + goog_validate_dob() + goog_validate_gender() + goog_validate_agreement() == 5){
				goog_report_conversion();
			}
		}
		// DO NOT CHANGE THE CODE BELOW.
		goog_report_conversion = function(url) {
			goog_snippet_vars();
			window.google_conversion_format = "3";
			var opt = new Object();
			opt.onload_callback = function() {
				if (typeof(url) != 'undefined') {
				  window.location = url;
				}
			}
			var conv_handler = window['google_trackConversion'];
			if (typeof(conv_handler) == 'function') {
				conv_handler(opt);
			}
		}
		/*  */
	</script>
